import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a==1)
            System.out.println("Monday");
        else if(a==7)
             System.out.println("Sunday");
        else
             System.out.println("Enter a valid Input");
    }
}
